{
  "name": "leveleditor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "nodemon server.js -i src -i public",
    "clean": "rimraf public/js public/css",
    "make-public": "npm run clean && mkdirp public/js && mkdirp public/css",
    "build-js": "node tasks/make-js.js",
    "prod-js": "node tasks/make-js.js --minify",
    "watch-js": "node tasks/make-js.js -w",
    "build-sass": "node tasks/make-sass.js",
    "watch-sass": "node tasks/make-sass.js -w",
    "build": "npm run make-public && npm run build-sass && npm run build-js",
    "prod": "npm run make-public && npm run prod-js",
    "livereload_old": "livereload public -i 200",
    "reload": "node tasks/livereload.js",
    "start-prod": "npm run prod && npm run minify-js && npm run serve",
    "start": "npm run watch-sass & npm run watch-js & npm run serve & npm run reload"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "plugins": [
            "object-assign"
          ]
        }
      ],
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "author": "Shaun West",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-plugin-object-assign": "^1.2.1",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "copyfiles": "^0.2.1",
    "file": "^0.2.2",
    "gaze": "^0.5.2",
    "http-server": "^0.8.0",
    "livereload": "^0.3.7",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.3.3",
    "ractive": "^0.7.3",
    "reactify": "^1.1.1",
    "rimraf": "^2.4.2",
    "uglify-js": "^2.5.0",
    "uglifyjs": "^2.4.10",
    "watch": "^0.16.0",
    "watchify": "^3.3.1",
    "yargs": "^3.27.0"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "express": "^4.13.3",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.1.1",
    "page": "^1.6.3",
    "react": "^0.14.0",
    "react-addons-create-fragment": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.2",
    "redux-immutablejs": "0.0.6",
    "redux-logger": "^1.0.5",
    "redux-thunk": "^0.1.0"
  }
}
